{
    "multipart": [
        {   "when": { "up": true },
            "apply": { "model": "mossy_cobblestone_wall_post", "uvlock": true }
        },
        {   "when": { "north": true },
            "apply": { "model": "mossy_cobblestone_wall_side", "uvlock": true }
        },
        {   "when": { "east": true },
            "apply": { "model": "mossy_cobblestone_wall_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": true },
            "apply": { "model": "mossy_cobblestone_wall_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": true },
            "apply": { "model": "mossy_cobblestone_wall_side", "y": 270, "uvlock": true }
        },
        {   "when": { "north_east": true },
            "apply": { "model": "mossy_cobblestone_wall_diagonal"}
        },
        {   "when": { "south_east": true },
            "apply": { "model": "mossy_cobblestone_wall_diagonal", "y": 90 }
        },
        {   "when": { "south_west": true },
            "apply": { "model": "mossy_cobblestone_wall_diagonal", "y": 180 }
        },
        {   "when": { "north_west": true },
            "apply": { "model": "mossy_cobblestone_wall_diagonal", "y": 270 }
        }
    ]
}
